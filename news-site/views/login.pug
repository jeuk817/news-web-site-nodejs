extends sign

block content
    div(id="loginContainer")
        input(type="text" id="inputID" placeholder="아이디")
        p(id="noteID")
        input(type="password" id="inputPwd" placeholder="비밀번호")
        p(id="notePwd")
        input(type="submit" id="loginBtn" value="로그인")
        p(id="noteLogin")
        input(type="button" id="signUpBtn" value="회원가입")